<?xml version="1.0" encoding="UTF-8" ?>

<routes xmlns="http://symfony.com/schema/routing"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://symfony.com/schema/routing http://symfony.com/schema/routing/routing-1.0.xsd">

    <import resource="@FOSUserBundle/Resources/config/routing/all.xml"/>

    <route id="homepage" path="/">
        <default key="_controller"></default>
    </route>

    <route id="public.login" path="/login">
        <default key="_controller">AppBundle:User:login</default>
    </route>

    <route id="admin.home" path="/admin">
        <default key="_controller">AppBundle:Admin:courseList</default>
    </route>

</routes>



