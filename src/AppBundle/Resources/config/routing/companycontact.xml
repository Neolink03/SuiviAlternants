<?xml version="1.0" encoding="UTF-8" ?>

<routes xmlns="http://symfony.com/schema/routing"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/routing http://symfony.com/schema/routing/routing-1.0.xsd">

    <route id="companycontact_index" path="/" methods="GET">
        <default key="_controller">AppBundle:CompanyContact:index</default>
    </route>

    <route id="companycontact_show" path="/{id}/show" methods="GET">
        <default key="_controller">AppBundle:CompanyContact:show</default>
    </route>

    <route id="companycontact_new" path="/new" methods="GET|POST">
        <default key="_controller">AppBundle:CompanyContact:new</default>
    </route>

    <route id="companycontact_edit" path="/{id}/edit" methods="GET|POST">
        <default key="_controller">AppBundle:CompanyContact:edit</default>
    </route>

    <route id="companycontact_delete" path="/{id}/delete" methods="DELETE">
        <default key="_controller">AppBundle:CompanyContact:delete</default>
    </route>

</routes>
